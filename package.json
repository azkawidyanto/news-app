{
	"name": "news-app",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^13.0.0",
		"@testing-library/user-event": "^13.2.1",
		"axios": "^1.3.6",
		"dotenv": "^16.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dotenv": "^0.1.3",
		"react-router": "^6.10.0",
		"react-router-dom": "^6.10.0",
		"react-scripts": "5.0.1",
		"web-vitals": "^2.1.0"
