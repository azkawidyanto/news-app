			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"react-dotenv": {
		"whitelist": [
			"API_KEY"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/plugin-syntax-jsx": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"babel-jest": "^29.5.0"
	},
	"jest": {
		"moduleNameMapper": {
			"axios": "axios/dist/node/axios.cjs"
		},
		"testEnvironment": "jsdom"
	}
